/*
Your task is to make two functions, max and min (maximum and minimum in PHP and Python) that take 
a(n) array/vector of integers list as input and outputs, respectively, 
the largest and lowest number in that array/vector./*

using System;
using System.Linq;

public class Kata
{
        public int Min(int[] list)
        {
            return list.Min();
        }

        public int Max(int[] list)
        {
            return list.Max();
        }
}
